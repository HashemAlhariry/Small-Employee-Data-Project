<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:controller="com.example.sample.controller.EmployeeController" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" stylesheets="@application.css">
            <children>
                <AnchorPane layoutX="2.0" prefHeight="373.0" prefWidth="640.0">
                    <children>
                        <Label layoutX="85.0" layoutY="15.0" prefHeight="28.0" prefWidth="85.0" text="Person Details" stylesheets="@application.css"/>
                        <Label layoutX="100.0" layoutY="60.0" prefHeight="17.0" prefWidth="18.0" text="ID" />
                        <Label layoutX="100.0" layoutY="100.0" prefHeight="17.0" prefWidth="66.0" text="First Name" />
                        <Label layoutX="100.0" layoutY="140.0" text="Age" />
                        <Label layoutX="100.0" layoutY="180.0" prefHeight="5.0" prefWidth="73.0" text="Department" />


                        <Button layoutX="151.0" layoutY="310.0" mnemonicParsing="false" text="Update" fx:id="updateUser" onMouseClicked="#updateUser" />
                        <Button layoutX="233.0" layoutY="310.0" mnemonicParsing="false" text="Delete" fx:id="deleteUser"  onMouseClicked="#deleteUser" />
                        <Button layoutX="365.0" layoutY="310.0" mnemonicParsing="false" text="First"   fx:id="firstUser" onMouseClicked="#getFirstUser" />
                        <Button layoutX="421.0" layoutY="310.0" mnemonicParsing="false" text="Pervious" fx:id="previousUser"  onMouseClicked="#getPreviousUser" />
                        <Button layoutX="494.0" layoutY="310.0" mnemonicParsing="false" text="Next"  fx:id="nextUser"  onMouseClicked="#getNextUser" />
                        <Button layoutX="549.0" layoutY="310.0" mnemonicParsing="false" text="Last" fx:id="lastUser" onMouseClicked="#getLastUser"/>

                        <TextField layoutX="250.0" layoutY="60.0"   fx:id="id" />
                        <TextField layoutX="250.0" layoutY="100.0"  fx:id="firstName"  />
                        <TextField layoutX="250.0" layoutY="140.0"  fx:id="age" />
                        <TextField layoutX="250.0" layoutY="180.0"  fx:id="department" />

                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </children>
</VBox>
